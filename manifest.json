{
  "manifest_version": 3,
  "name": "YouTube Essentials",
  "version": "1.1",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["categoryBlocker.js", "eduOrEnt.js"]
    }
  ]
}
  